<dependency>
    <groupId>org.springframework</groupId>
    <artifactId>spring-web</artifactId>
    <version>4.3.3.RELEASE</version>
</dependency>
<dependency>
<groupId>com.niit</groupId>
  <artifactId>ShoppingPortal</artifactId>
  <version>0.0.1-SNAPSHOT</version>
